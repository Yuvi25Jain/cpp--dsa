int main()
{
    int n;
    cout<<"enter the value of n"<<endl;
    cin>>n;
    
   
    int i=0;
    int ans =0;
    while(n!=0){
        int digit = n%10; //last digit of number or breaking of number
        if (digit==1){
            ans = ans + pow(2,i);
            
        }
        n = n/10;
        i++;
    }
    
    cout<<ans<<endl;
    
    return 0; 
}

