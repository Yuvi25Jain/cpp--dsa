#include <iostream>
using namespace std;


int main()
{
    int n , rem , rev = 0, temp;
    
    cout<<"enter no to check: "<<endl; 
    cin>>n;
    temp = n;
    
    while(n!=0){  
        rem = n % 10;
        n = n /10;
        rev = rev*10 + rem;
        
    }
    if(temp == rev){
        cout<<"Palindrome";
        
    }
    
    else{
        cout<<"not palindrome";
        
    }
    return 0;
}
