#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    int n;  \\ taking input from user
    cout<<"enter the value of n"<<endl;
    cin>>n;
    
    int rem;
    int a[10];
    int i=0;
    int j;
    while(n!=0){
        rem = n%2; //remainder
        n = n/2; // dividing
        a[i]= rem;
        i++;
    }
    for (j = i-1; j>=0; j--){
        cout<<a[j];
    }
    return 0; 
}

