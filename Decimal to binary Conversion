#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    int n;
    cout<<"enter the value of n"<<endl;
    cin>>n;
    
    int rem;
    int a[10];
    int i=0;
    int j;
    while(n!=0){
        rem = n%2;
        n = n/2;
        a[i]= rem;
        i++;
    }
    for (j = i-1; j>=0; j--){
        cout<<a[j];
    }
    return 0; 
}

