/***1 for true yeah this is prime number and 0 for false this is not a prime number**/

bool isPrimeNumber(int n){
    /**if the number divides not a prime number therefore return 0**/;
    for(int i=2; i<n;i++){
        if(n%2==0){
            return 0;
        }
        return 1;
    }
}

int main(){
    int n;
    cout<<"enter the sample number"<<endl;
    cin>>n;
    
    if (isPrimeNumber(n)){
        cout<<"prime"<<endl;
    }
    else{
        cout<<" not a prime"<<endl;
