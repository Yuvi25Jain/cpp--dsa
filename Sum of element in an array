int sumArray(int arr[], int n){
    int sum = 0;
    for (int i=0; i<n ; i++){
        sum+= arr[i];
    }
    return sum;
}

int main()
{
   int arr[100];
   int n;
   cout<<"enter the size of array"<<endl;
   cin>>n;
   
   for(int i=0; i<n; i++){
       cout<<"enter the elements of array"<<endl;
       cin>> arr[i];
   }
   
   cout<<"sum of the elements of array is here:  "<< sumArray(arr, n)<<endl;
    return 0;
}
